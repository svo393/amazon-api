    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.3",
    "@types/nodemailer": "^6.4.0",
    "@types/ramda": "^0.27.6",
    "@types/sharp": "^0.25.0",
    "@types/supertest": "^2.0.9",
