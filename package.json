  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "init": "cross-env NODE_ENV=development ts-node ./db/init.ts",
    "knex": "knex",
    "lint": "eslint **/*.ts --ignore-pattern node_modules/",
