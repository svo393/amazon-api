    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "keywords": [],
  "license": "ISC",
  "main": "src/index.ts",
  "name": "amazon-api",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEBUG=knex:query ts-node-dev src/index.ts",
    "init": "cross-env NODE_ENV=development ts-node ./db/init.ts",
    "knex": "knex",
    "migrate:down": "knex migrate:down --knexfile ./db/knexfile.ts",
    "migrate:up": "knex migrate:up --knexfile ./db/knexfile.ts && cross-env NODE_ENV=development ts-node ./db/init.ts",
    "seed": "cross-env NODE_ENV=development jest ./tests/seed.test.ts --verbose --runInBand",
    "seed:make": "knex seed:make seed_name",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "test": "cross-env NODE_ENV=development jest --verbose --runInBand",
    "test:acomm": "cross-env NODE_ENV=development jest ./tests/answerCommentsAPI.test.ts --verbose  --runInBand",
    "test:addr": "cross-env NODE_ENV=development jest ./tests/addressesAPI.test.ts --verbose  --runInBand",
    "test:answers": "cross-env NODE_ENV=development jest ./tests/answersAPI.test.ts --verbose  --runInBand",
    "test:at": "cross-env NODE_ENV=development jest ./tests/addressTypesAPI.test.ts --verbose  --runInBand",
    "test:cart": "cross-env NODE_ENV=development jest ./tests/cartProductsAPI.test.ts --verbose  --runInBand",
    "test:cache": "jest --clear-cache",
    "test:cats": "cross-env NODE_ENV=development jest ./tests/categoriesAPI.test.ts --verbose  --runInBand",
    "test:dev": "cross-env NODE_ENV=development jest --verbose --runInBand --watch",
    "test:follow": "cross-env NODE_ENV=development jest ./tests/followersAPI.test.ts --verbose  --runInBand",
    "test:groups": "cross-env NODE_ENV=development jest ./tests/groupsAPI.test.ts --verbose  --runInBand",
    "test:lists": "cross-env NODE_ENV=development jest ./tests/listsAPI.test.ts --verbose  --runInBand",
    "test:lp": "cross-env NODE_ENV=development jest ./tests/listProductsAPI.test.ts --verbose  --runInBand",
    "test:params": "cross-env NODE_ENV=development jest ./tests/parametersAPI.test.ts --verbose  --runInBand",
    "test:products": "cross-env NODE_ENV=development jest ./tests/productsAPI.test.ts --verbose  --runInBand",
    "test:pt": "cross-env NODE_ENV=development jest ./tests/paymentTypesAPI.test.ts --verbose  --runInBand",
    "test:questions": "cross-env NODE_ENV=development jest ./tests/questionsAPI.test.ts --verbose  --runInBand",
    "test:ratings": "cross-env NODE_ENV=development jest ./tests/ratingsAPI.test.ts --verbose  --runInBand",
    "test:rcomm": "cross-env NODE_ENV=development jest ./tests/ratingCommentsAPI.test.ts --verbose  --runInBand",
    "test:roles": "cross-env NODE_ENV=development jest ./tests/rolesAPI.test.ts --verbose  --runInBand",
    "test:sm": "cross-env NODE_ENV=development jest ./tests/shippingMethodsAPI.test.ts --verbose  --runInBand",
    "test:useraddr": "cross-env NODE_ENV=development jest ./tests/userAddressesAPI.test.ts --verbose  --runInBand",
    "test:users": "cross-env NODE_ENV=development jest ./tests/usersAPI.test.ts --verbose  --runInBand",
    "test:vendors": "cross-env NODE_ENV=development jest ./tests/vendorsAPI.test.ts --verbose  --runInBand",
    "tsc": "rm -rf build && tsc",
    "tut": "ts-node-dev src/tut.ts"
  },
  "version": "1.0.0"
}
