  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "tut": "ts-node-dev src/tut.ts",
    "init": "cross-env NODE_ENV=development ts-node ./db/init.ts",
    "seed:make": "knex seed:make seed_name",
    "migrate:up": "knex migrate:up --knexfile ./db/knexfile.ts && cross-env NODE_ENV=development ts-node ./db/init.ts",
    "migrate:down": "knex migrate:down --knexfile ./db/knexfile.ts",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "seed": "cross-env NODE_ENV=test jest ./tests/seed.test.ts --verbose --runInBand",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "test:vendors": "cross-env NODE_ENV=test jest ./tests/vendorsAPI.test.ts --verbose  --runInBand",
    "test:cats": "cross-env NODE_ENV=test jest ./tests/categoriesAPI.test.ts --verbose  --runInBand",
    "test:items": "cross-env NODE_ENV=test jest ./tests/itemsAPI.test.ts --verbose  --runInBand",
    "test:users": "cross-env NODE_ENV=test jest ./tests/usersAPI.test.ts --verbose  --runInBand",
    "test:dev": "cross-env NODE_ENV=test jest --verbose --runInBand --watch",
    "tsc": "rm -rf build && tsc"
  },
